{
  "name": "Ableton AI Chatbot",
  "type": "module",
  "private": true,
  "version": "0.0.2",
  "description": "An AI-powered assistant for Ableton Live documentation",
  "author": {
    "name": "Todd Greco",
    "email": "mrballistic@gmail.com"
  },
  "main": ".vite/build/main.cjs",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "start": "electron-forge start",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && cross-env NODE_ENV=development electron-forge start\"",
    "prebuild": "webpack --config webpack.main.config.cjs",
    "package": "npm run prebuild && node --no-deprecation ./node_modules/.bin/electron-forge package",
    "make": "npm run prebuild && node --no-deprecation ./node_modules/.bin/electron-forge make",
    "publish": "electron-forge publish",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run",
    "test:backend": "node --experimental-vm-modules node_modules/jest/bin/jest.js server.test.js",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@langchain/community": "^0.0.19",
    "@langchain/core": "^0.1.48",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "chromadb": "^1.7.3",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.18.2",
    "follow-redirects": "^1.15.9",
    "hnswlib-node": "^1.4.2",
    "langchain": "^0.0.213",
    "markdown-it": "^14.1.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-modules-commonjs": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.0",
    "@babel/runtime-corejs3": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron-forge/plugin-webpack": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.36.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "electron": "^33.3.1",
    "electron-installer-dmg": "^5.0.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "path-browserify": "^1.0.1",
    "supertest": "^6.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "wait-on": "^8.0.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".js"
    ]
  },
  "vitest": {
    "environment": "jsdom",
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "globals": true,
    "css": true
  },
  "config": {
    "forge": "./forge.config.cjs"
  }
}
